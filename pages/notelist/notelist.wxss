/* pages/oxnotes/oxnotes.wxss */
/* 头部 */
.head{box-sizing: border-box;margin-bottom: 30rpx;position: relative;padding: 29rpx 0;height: 88rpx;background-color: #fff;border-bottom: 2rpx solid #dddddd;z-index: 9999}
.head_condition_noe,.head_condition_two{box-sizing: border-box;position: relative;width: 50%}
.head_condition_noe{padding-left: 126rpx;}
.head_condition_two{padding-left: 106rpx;}
.head_condition_option{position: relative;line-height: 30rpx;font-size: 30rpx;color: #666}
/* 上三角形 */
.head_icon_trigon{position: absolute;top: -14rpx;width:0;height:0;border-width:0 12rpx 12rpx;border-style:solid;border-color:transparent transparent #dddddd}
.head_icon_trigon_one{left: 176rpx}
.head_icon_trigon_two{left: 498rpx}
/* 选择列表 */
.head_list{position: absolute;width: 50%;background-color: #fff;border-bottom: 2rpx solid #dddddd;z-index: 9999999999999999999999}
.head_list_option_one {
  left: 0;
}
.head_list_option_two {
  right: 0;
}
.head_list_box{box-sizing: border-box;height: 90rpx;border-bottom: 2rpx solid #eee}
.head_list_box:last-child {
  border: none;
}
.head_list_box_option{width: 100%;text-align: center;line-height: 90rpx;font-size: 28rpx;color: #666}
/* icon：success_no_circle 组件样式调整 */
.success_no_circle{position: relative;top: -4rpx}
/* 选中灰色背景 */
.bc_gray{background-color: #f5f5f9}
/* 选中蓝色字体 */

.selecte_item {
  color: #3F86FF;
}

.bc_none{background-color: #fff}
/* 笔记列表 */
.killer{margin-top: 27rpx;padding-bottom: 35rpx;background-color: #fff}
.killer_section{padding: 0 30rpx}
.killer_section_box{box-sizing: border-box;position: relative;margin-top: 20rpx;padding: 30rpx 30rpx 34rpx 0;width: 680rpx;border: 3rpx solid #dddddd;border-top-left-radius: 30rpx;border-top-right-radius: 30rpx;background-color: #fff}
.killer_section_box:first-child{margin-top: 10rpx}
.killer_section_icon_line{position: absolute;top: 50%;left: 260rpx;margin-top: -75rpx;width: 2rpx;height: 160rpx;background-color: #dddddd}
.killer_section_icon_buy{position: absolute;top:30rpx;right: -12rpx;width: 140rpx;height: 50rpx;line-height: 50rpx;border-top-left-radius: 25rpx;border-bottom-left-radius: 25rpx;font-size: 24rpx;text-align: center;color: #fff}
.killer_section_icon_trigon{position: absolute;top: 78rpx;right: -12rpx;width: 0;height: 0;border-right: 11rpx solid #f5f5f9}
.buy_false{background-color: #dddddd}
.trigon_false{border-top: 11rpx solid #dddddd;}
.buy_true{background-color: #ff9b74}
.trigon_true{border-top: 12rpx solid #ff9b74}
.killer_section_box_left{float: left;width: 260rpx;box-sizing: border-box;}
.killer_section_box_left_userHead{border: 2rpx solid #eee;display: block;margin: 0 auto;width: 83rpx;height: 83rpx;border-radius: 50%;}
.killer_section_box_left_userName{margin: 18rpx auto ;width: 70%;font-size: 26rpx;color: #3e3e3e;text-align: center}
.killer_section_box_left_depict{width: 100%;font-size: 24rpx;line-height: 24rpx;color: #999;text-align: center}
.killer_section_box_right{float: left;width: 387rpx;padding-left: 30rpx;box-sizing: border-box;}
.killer_section_box_right_one_title{margin-bottom: 6rpx;font-size: 38rpx;color: #141414;line-height: 38rpx;width: 200rpx}
.killer_section_box_right_one_depict{float: left;margin-top: 20rpx;font-size: 24rpx;color: #aac0ff;line-height: 24rpx}
.killer_sectsion_box_right_one_title {font-size: 36rpx;line-height: 36rpx;}
.killer_section_box_right_two_left{float: left}
.killer_section_box_right_two_left_title{width: 225rpx;font-size: 36rpx;color: #e32f37;line-height: 36rpx;margin-top: 30rpx;}
.killer_section_box_right_two_left_depict{font-size: 26rpx;color: #999;line-height: 26rpx;margin-top: 20rpx;}
.killer_section_box_right_two_right{float: right}
.killer_section_box_right_two_right_title{font-size: 36rpx;color: #ffad2b;text-align: center}
.killer_section_box_right_two_right_depict{font-size: 26rpx;color: #999;line-height: 26rpx;margin-top: 15rpx;}
/* 箭头：下 */
.icon_bottom_arrow{position: relative;margin-left: 20rpx;top: 6rpx;width: 34rpx;height: 19rpx}
.needPriceYuanbao {
  display: inline-block;
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  top: 67%;
  transform: translateY(-50%); 
}
.needPriceText{
  display: inline-block;
  font-size:36rpx;
  color:#FC7946;
  text-align:center;
  line-height: 33rpx;
  padding-left: 50rpx;
}
.freePriceText{
  display: inline-block;
  font-size:36rpx;
  color:#FC7946;
  line-height: 32rpx;
  text-align:center;
}
.seal {
  width: 68rpx;
  height: 54rpx;
  position: absolute;
  top:110rpx;
  left:179rpx;
}
.seal image {
  width: 68rpx;
  height: 54rpx;
}
.pstRelative {
  position: relative;
}
.detailMask {
  position: fixed;
  top: 88rpx;
  left: 0;
  width: 100%;
  height: 93vh;
  background-color: rgba(0, 0, 0, 0.5);
  z-index: 999999999;
}