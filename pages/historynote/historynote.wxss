/* 列表 */
.list .box{margin-top: 20rpx;background-color: #fff;font-size: 0}
.list .box:first-child{margin: 0}
.list .box .one{margin-bottom: 30rpx;padding:20rpx 30rpx;border-bottom: 2rpx solid #eeeeee}
/* .list .box .one .left text{font-weight: 400} */
.share_text_box{margin-right: 10rpx;font-size: 36rpx;color: #333;line-height: 0;font-weight: bold}
.list .box .one .left text:nth-child(2){font-size: 32rpx;color: #fc7946;line-height: 36rpx}
.list .box .one .right{font-size: 26rpx;color: #b9b9b9;height: 36rpx;line-height: 36rpx}
.list .box .two{margin-bottom: 30rpx}
.list .box .two>view{width: 49.8%;text-align: center}
.list .box .two>view:first-child{border-right: 1px solid #dddddd}
.list .box .two>view>view:nth-child(1){margin-bottom: 20rpx;font-size: 40rpx;color: #e32f37;line-height: 40rpx;font-weight: 600}
.list .box .two>view>view:nth-child(2){font-size: 26rpx;color: #666;line-height: 26rpx}
.list .box .three{font-size: 26rpx;line-height: 26rpx;padding: 0 30rpx 30rpx 30rpx;}
.list .box .three>view:nth-child(1){color: #666}
.list .box .three>view:nth-child(2){color: #808080}
.list .box .three>view:nth-child(2) .icon{margin-right: 20rpx;width: 32rpx;height: 28rpx;vertical-align: middle}
.list .box .three>view:nth-child(2) text{vertical-align: middle}
