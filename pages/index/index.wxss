/**index.wxss**/
/* 导航 */
.nav{padding: 30rpx 0;background-color: #fff}
.nav_icon{width: 100rpx;height: 100rpx; display: inline-block;}
.nav_icon image{width: 100%;height: 100%}
.nav navigator{ width: 25%;float:left;text-align: center;}
.nav-btm-text {
  font-size: 26rpx;
  color: #666;
  margin-top: 10rpx;
}
.nav-swiper-box {
  width: 690rpx;
  height: 180rpx;
  padding: 25rpx 30rpx 0;
}
.nav-swiper-box swiper {
  width: 100%;
  height: 100%;
}
.nav-swiper-item image{
  width: 690rpx;
  height: 180rpx;
}
/* 头部 */
.head{box-sizing: border-box;position: relative;margin: -45rpx auto 0;padding: 36rpx 50rpx 0;width: 690rpx;height:160rpx;border-radius:10rpx;background-color: #fff}
.head_icon{position: absolute;top: 50%;left: 50%;margin-top: -36rpx;margin-left: -1rpx;width: 2rpx;height: 72rpx;background-color: #dddddd}
.head_left{float: left;width: 50%}
.head_left_icon{float: left;margin-right: 20rpx;width: 93rpx;height: 93rpx}
.head_left_title{float: left;font-size: 36rpx;color: #141414;line-height: 93rpx}
.head_right{float: right;}
/* 热门笔记原先1.0版本 */
.hot{margin-top: 20rpx;margin-bottom: 20rpx;background-color: #fff}
.hot_scroll{white-space: nowrap;height: 300rpx}
.scroll-view-item{display: inline-block;width: 750rpx;height: 281rpx}
.hot_title{box-sizing: border-box;padding: 25rpx 0;width:100%;}
.hot_title_icon{margin: 0 20rpx 0 30rpx;width: 10rpx;height: 30rpx;background-color: #2772ff}
.hot_title_text{position: relative;top:-2.5rpx;font-size: 32rpx;color: #141414}
.hot_title_more{font-size: 26rpx;color: #b9b9b9}
.hot_section{box-sizing: border-box;padding: 0 15rpx;height: 281rpx}
.hot_section_daub_one{margin: 0 auto;width: 599rpx;height: 27rpx;background-color: #f8f8f8;border-top-left-radius: 15rpx;border-top-right-radius: 15rpx}
.hot_section_daub_two{margin: -12rpx auto 0;width: 632rpx;height: 12rpx;background-color: rgba(235,235,235,0.5);border-top-left-radius: 10rpx;border-top-right-radius: 10rpx}
.hot_section_box{box-sizing: border-box;margin-top:5rpx;padding: 30rpx 40rpx;width: 650rpx;box-shadow: 0rpx 0rpx 4rpx #666;border-radius: 30rpx;}
.hot_section_box_modular_one{border-bottom: 2rpx solid #dddddd;padding-bottom: 30rpx;}
.hot_section_box_modular_one_left{float: left;width: 414rpx;border-right: 2rpx solid #dddddd}
.hot_section_box_modular_one_left_title{float: left;margin-right: 38rpx;line-height: 76rpx;font-size: 36rpx;font-weight: 400;color: #3e3e3e}
.hot_section_box_modular_one_left_depict{float: left}
.hot_section_box_modular_one_left_depict_one{font-size: 36rpx;font-weight: 600;line-height: 36rpx;color: #e32f37;margin-bottom: 20rpx;}
.hot_section_box_modular_one_left_depict_two{font-size: 26rpx;color: #808080;line-height: 26rpx;}
.hot_section_box_modular_one_right{float: right;margin-right: 10rpx;text-align: right}
.hot_section_box_modular_one_right_one{font-size: 36rpx;color: #fc7946;text-align: center;margin-bottom: 20rpx;line-height: 36rpx;}
.hot_section_box_modular_one_right_two{font-size: 26rpx;color: #808080;line-height: 26rpx}
.hot_section_box_modular_two{box-sizing: border-box;padding: 26rpx 0 0 0;}
.hot_section_box_modular_two_userHead{float: left;margin: 0 20rpx 0 0;width: 48rpx;height: 48rpx;border-radius: 50%;}
.hot_section_box_modular_two_userName{float: left;margin: 0 10rpx 0 0;width: 350rpx;height: 48rpx;line-height: 48rpx;overflow:hidden;font-size: 28rpx}
.hot_section_box_modular_two_depict{float: left;width: 230rpx;height: 48rpx;line-height: 48rpx;font-size: 26rpx;color: #808080}
.hot_section_box_modular_two_btn{float: right;width: 140rpx;height: 50rpx;line-height: 50rpx;font-size: 24rpx;color: #fff;background-color: #ff9b74;text-align: center;border-radius: 24rpx}

