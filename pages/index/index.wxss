/* 热门观点 */
.view{background-color: #fff}
.view .section{padding: 0 30rpx}
.view .section .box:last-child{border: 0}
.view .section .box .title{font-size: 36rpx;color: #2e2e2e;font-weight: 600;line-height: 40rpx;}
.view .section .box .text{margin: 26rpx 0;line-height: normal;font-size: 28rpx;color: #808080}
.view .section .box .info{height:52rpx}
.view .section .box .info .headimg{margin:0 20rpx 0 0;width:49rpx;height:49rpx;border-radius:50%;vertical-align: middle}
.view .section .box .info .name{display: inline-block;font-size: 26rpx;color: #666}
.view .section .box .info .data .icon{margin-right: 20rpx;width: 40rpx;height: 35rpx;vertical-align: middle}
.view .section .box .info .data .number{font-size: 24rpx;color: #999}
.view .section .box .info .data .number:nth-child(2){margin-right: 49rpx}

/* 高手笔记 */
.killer{padding-bottom: 35rpx;background-color: #fff}
.killer_section{padding: 0 30rpx}
.killer_section_box{box-sizing: border-box;position: relative;margin-top: 20rpx;padding: 30rpx 19rpx 34rpx;width: 695rpx;height: 255rpx;border: 3rpx solid #dddddd;border-top-left-radius: 30rpx;border-top-right-radius: 30rpx}
.killer_section_box:first-child{margin-top: 0}
.killer_section_icon_line{position: absolute;top: 50%;left: 261rpx;margin-top: -75rpx;width: 2rpx;height: 150rpx;background-color: #dddddd}
.killer_section_icon_buy{position: absolute;top:45rpx;right: -12rpx;width: 140rpx;height: 50rpx;line-height: 50rpx;border-top-left-radius: 25rpx;border-bottom-left-radius: 25rpx;font-size: 24rpx;text-align: center;color: #fff}
.killer_section_icon_trigon{position: absolute;top: 95rpx;right: -12rpx;width: 0;height: 0;border-right: 12rpx solid #fff}
.buy_false{background-color: #dddddd}
.trigon_false{border-top: 12rpx solid #b9b9b9}
.buy_true{background-color: #ff9b74}
.trigon_true{border-top: 12rpx solid #e68d00}
.killer_section_box_left{float: left;width: 230rpx}
.killer_section_box_left_userHead{display: block;margin: 0 auto;width: 83rpx;height: 83rpx;border-radius: 50%;}
.killer_section_box_left_userName{margin: 20rpx auto;width: 70%;font-size: 28rpx;color: #333;text-align: center;line-height: 28rpx;}
.killer_section_box_left_depict{width: 100%;font-size: 24rpx;line-height: 24rpx;color: #999;text-align: center}
.killer_section_box_right{float: right;width: 376rpx;}
.killer_section_box_right_one_title{margin-bottom: 6rpx;font-size: 38rpx;color: #141414;line-height: 38rpx;width: 200rpx}
.killer_section_box_right_one_depict{float: left;margin-top: 20rpx;font-size: 24rpx;color: #aac0ff;line-height: 24rpx;}
.killer_sectsion_box_right_one_title {font-size: 36rpx;line-height: 36rpx;}
.killer_section_box_right_two_left{float: left}
.killer_section_box_right_two_left_title{width: 200rpx;font-size: 34rpx;line-height: 34rpx;color: #e32f37;margin-top: 30rpx;}
.killer_section_box_right_two_left_depict{font-size: 26rpx;line-height: 26rpx;margin-top: 20rpx;color: #999}
.killer_section_box_right_two_right{float: right}
.killer_section_box_right_two_right_title{color: #ffad2b;text-align: center}
.killer_section_box_right_two_right_depict{font-size: 26rpx;line-height: 26rpx;margin-top: 16rpx;color: #999}
