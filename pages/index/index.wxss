/* 问股 */
.view .section .box{padding: 30rpx 0;border-bottom: 1px solid #eeeeee}
.box{padding: 30rpx 30rpx 21rpx 30rpx;background-color: #fff}
.box:first-child{margin: 0}
.box .title image{margin-right: 20rpx;width: 48rpx;height: 48rpx;border-radius: 50%}
.box .title text:nth-child(2){font-size: 28rpx;color: #808080;line-height: 48rpx}
.box .title text:nth-child(3){font-size: 36rpx;color: #fc7946;line-height: 48rpx}
