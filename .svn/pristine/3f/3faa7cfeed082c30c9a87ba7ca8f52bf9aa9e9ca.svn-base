/**index.wxss**/
/* 导航 */
.nav{padding: 56rpx 30rpx 0;height: 180rpx;background-color: #2772ff}
.nav_icon{width: 81rpx;height: 94rpx; display: inline-block;}
.nav_icon image{width: 100%;height: 100%}
.nav navigator{ width: 25%;float:left;text-align: center;}
/* 头部 */
.head{box-sizing: border-box;position: relative;margin: -45rpx auto 0;padding: 36rpx 50rpx 0;width: 690rpx;height:160rpx;border-radius:10rpx;background-color: #fff}
.head_icon{position: absolute;top: 50%;left: 50%;margin-top: -36rpx;margin-left: -1rpx;width: 2rpx;height: 72rpx;background-color: #dddddd}
.head_left{float: left;width: 50%}
.head_left_icon{float: left;margin-right: 20rpx;width: 93rpx;height: 93rpx}
.head_left_title{float: left;font-size: 36rpx;color: #141414;line-height: 93rpx}
.head_right{float: right;}
/* 热门笔记 */
.hot{margin-top: 20rpx;margin-bottom: 20rpx;height: 420rpx;background-color: #fff}
.hot_scroll{white-space: nowrap;height: 300rpx}
.scroll-view-item{display: inline-block;width: 750rpx;height: 281rpx}
.hot_title{box-sizing: border-box;padding-top: 40rpx;width:100%;height: 110rpx}
.hot_title_icon{margin: 0 20rpx 0 30rpx;width: 10rpx;height: 30rpx;background-color: #2772ff}
.hot_title_text{position: relative;top:-2.5rpx;font-size: 32rpx;color: #141414}
.hot_title_more{font-size: 26rpx;color: #b9b9b9}
.hot_section{box-sizing: border-box;padding: 0 20rpx;height: 281rpx}
.hot_section_daub_one{margin: 0 auto;width: 599rpx;height: 27rpx;background-color: #f8f8f8;border-top-left-radius: 15rpx;border-top-right-radius: 15rpx}
.hot_section_daub_two{margin: -12rpx auto 0;width: 632rpx;height: 12rpx;background-color: rgba(235,235,235,0.5);border-top-left-radius: 10rpx;border-top-right-radius: 10rpx}
.hot_section_box{box-sizing: border-box;margin-top:5rpx;padding: 40rpx 40rpx 23rpx;width: 650rpx;height: 255rpx;box-shadow: 0rpx 0rpx 4rpx #666;border-radius: 30rpx;}
.hot_section_box_modular_one{height: 114rpx;border-bottom: 2rpx solid #dddddd}
.hot_section_box_modular_one_left{float: left;width: 414rpx;height: 80rpx;border-right: 2rpx solid #dddddd}
.hot_section_box_modular_one_left_title{float: left;margin-right: 38rpx;line-height: 76rpx;font-size: 36rpx;font-weight: 400;color: #3e3e3e}
.hot_section_box_modular_one_left_depict{float: left}
.hot_section_box_modular_one_left_depict_one{font-size: 34rpx;font-weight: 400;color: #e32f37;margin-bottom: 10rpx;}
.hot_section_box_modular_one_left_depict_two{font-size: 26rpx;color: #808080}
.hot_section_box_modular_one_right{float: right;margin-right: 10rpx;text-align: right}
.hot_section_box_modular_one_right_one{font-size: 36rpx;color: #fc7946;text-align: center;margin-bottom: 10rpx;}
.hot_section_box_modular_one_right_two{font-size: 26rpx;color: #808080}
.hot_section_box_modular_two{box-sizing: border-box;padding: 26rpx 0 0 0;height: 74rpx}
.hot_section_box_modular_two_userHead{float: left;margin: 0 20rpx 0 0;width: 48rpx;height: 48rpx;border-radius: 6rpx;}
.hot_section_box_modular_two_userName{float: left;margin: 0 10rpx 0 0;width: 350rpx;height: 48rpx;line-height: 48rpx;overflow:hidden;font-size: 28rpx}
.hot_section_box_modular_two_depict{float: left;width: 230rpx;height: 48rpx;line-height: 48rpx;font-size: 26rpx;color: #808080}
.hot_section_box_modular_two_btn{float: right;width: 140rpx;height: 50rpx;line-height: 50rpx;font-size: 24rpx;color: #fff;background-color: #ff9b74;text-align: center;border-radius: 24rpx}
/* 热门观点 */
.view{background-color: #fff}
.view .section{padding: 0 30rpx}
.view .section .box{padding: 40rpx 0;border-bottom: 1px solid #eeeeee}
.view .section .box:last-child{border: 0}
.view .section .box .title{font-size: 36rpx;color: #2e2e2e;font-weight: 600;line-height: 36rpx;}
.view .section .box .text{margin: 36rpx 0;line-height: normal;font-size: 28rpx;color: #808080}
.view .section .box .info{height:49rpx}
.view .section .box .info .headimg{margin:0 20rpx 0 0;width:49rpx;height:49rpx;border-radius:50%;vertical-align: middle}
.view .section .box .info .name{display: inline-block;font-size: 26rpx;color: #666}
.view .section .box .info .data .icon{margin-right: 20rpx;width: 40rpx;height: 35rpx;vertical-align: middle}
.view .section .box .info .data .number{font-size: 24rpx;color: #999}
.view .section .box .info .data .number:nth-child(2){margin-right: 49rpx}

/* 高手笔记 */
.killer{padding-bottom: 35rpx;background-color: #fff}
.killer_section{padding: 0 30rpx}
.killer_section_box{box-sizing: border-box;position: relative;margin-top: 30rpx;padding: 40rpx 19rpx 34rpx;width: 695rpx;height: 255rpx;border: 3rpx solid #dddddd;border-top-left-radius: 30rpx;border-top-right-radius: 30rpx}
.killer_section_box:first-child{margin-top: 10rpx}
.killer_section_icon_line{position: absolute;top: 50%;left: 243rpx;margin-top: -75rpx;width: 2rpx;height: 150rpx;background-color: #dddddd}
.killer_section_icon_buy{position: absolute;top:45rpx;right: -12rpx;width: 140rpx;height: 50rpx;line-height: 50rpx;border-top-left-radius: 25rpx;border-bottom-left-radius: 25rpx;font-size: 24rpx;text-align: center;color: #fff}
.killer_section_icon_trigon{position: absolute;top: 95rpx;right: -12rpx;width: 0;height: 0;border-right: 12rpx solid #fff}
.buy_false{background-color: #dddddd}
.trigon_false{border-top: 12rpx solid #b9b9b9}
.buy_true{background-color: #ffad2b}
.trigon_true{border-top: 12rpx solid #e68d00}
.killer_section_box_left{float: left;width: 209rpx}
.killer_section_box_left_userHead{display: block;margin: 0 auto;width: 83rpx;height: 83rpx;border-radius: 10rpx}
.killer_section_box_left_userName{margin: 17rpx auto 8rpx;width: 70%;font-size: 28rpx;color: #3e3e3e;text-align: center}
.killer_section_box_left_depict{width: 100%;font-size: 24rpx;color: #b9b9b9;text-align: center}
.killer_section_box_right{float: right;width: 376rpx;}
.killer_section_box_right_one_title{margin-bottom: 6rpx;font-size: 38rpx;color: #141414;line-height: 38rpx;width: 200rpx}
.killer_section_box_right_one_depict{float: left;margin: 5rpx 20rpx 8rpx 0;font-size: 24rpx;color: #2772ff}
.killer_section_box_right_two_left{float: left}
.killer_section_box_right_two_left_title{width: 200rpx;font-size: 36rpx;color: #e32f37}
.killer_section_box_right_two_left_depict{font-size: 26rpx;color: #b9b9b9}
.killer_section_box_right_two_right{float: right}
.killer_section_box_right_two_right_title{font-size: 36rpx;color: #ffad2b;text-align: center}
.killer_section_box_right_two_right_depict{font-size: 26rpx;color: #b9b9b9}
/* 高手推荐 */
.recommend{padding: 0 0 35rpx;background-color: #fff;margin-top:20rpx;}
.recommend_scroll{padding: 0 30rpx}
.recommend_scroll_box{float: left;margin: 0 20rpx 0 0;padding: 30rpx 0;width: 213rpx;border: 2rpx solid #dddddd;border-radius: 20rpx}
 .recommend_scroll_box:last-child{margin-right: 0} 
.recommend_scroll_box_userHaed{display: block;margin: 0 auto;width: 84rpx;height: 84rpx;border-radius: 6rpx;}
.recommend_scroll_box_userName{margin: 20rpx auto;width: 70%;font-size: 26rpx;color: #333333;text-align: center}
.recommend_scroll_box_depict{width: 100%;font-size: 26rpx;color: #666;text-align: center}
.recommend_scroll .btn{margin: 30rpx auto 0;width: 88rpx;height: 38rpx;font-size: 22rpx;color: #e32f37;border: 1px solid #e32f37;border-radius: 20rpx;text-align: center;line-height: 37rpx}
.index_swiper_shareName {
  color: #141414;
  font-size:34rpx;
  margin-bottom: 10rpx;
}
.index_swiper_shareCode {
  color: #808080;
  font-size:26rpx;
}
.inde_swiper_shareInfo_box {
  float:left;
  margin-right:38rpx;
  font-weight:400;
  text-align: center;
}
.needPriceYuanbao {
  width: 44rpx;
  height: 44rpx;
  position: absolute;
  top: 47%;
  transform: translateY(-50%);
}
.needPriceText{
  padding-left: 50rpx;
  font-size:36rpx;
  color:#fc7946;
  text-align:center;
}
.freePriceText{
  font-size:36rpx;
  color:#fc7946;
  text-align:center;
}