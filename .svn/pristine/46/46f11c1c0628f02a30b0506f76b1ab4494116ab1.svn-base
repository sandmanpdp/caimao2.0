/* pages/market/market.wxss */
.goodsContainer {
  height: 100%;
  padding: 15rpx 10rpx 15rpx 15rpx;
}
.goodsBox {
  box-sizing:border-box;
  background: #fff;
  margin: 15rpx;
  padding: 30rpx 15rpx;
  width: 46%;
  height: 380rpx;
}
.picBox {
  height: 70%;
}
.picContent {
  width: 100%;
  height: 100%;
}
.goodsTitle {
  font-size: 27rpx;
