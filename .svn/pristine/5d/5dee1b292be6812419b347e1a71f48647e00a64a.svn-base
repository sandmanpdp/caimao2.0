view {
  box-sizing: border-box;
}
.coe32f37{color: #e32f37!important;font-weight: 600}
.cofc7946{color: #fc7946!important}
.co2772ff{color: #2772ff!important}
.cof4ebd4{color: #f4ebd4!important;background-color: #e32f37}
.lht45{line-height: 45rpx!important}
/* 头部 */
.head{height: 333rpx}
.head_top{box-sizing: border-box;padding: 40rpx 48rpx 38rpx;height: 184rpx;background-color: #2772ff}
.head_top_userHead{position: relative;width: 102rpx;height: 102rpx;border-radius: 10rpx;background-color: #fff;border: 2rpx solid #fff}
.head_top_userHead_img{width: 100%;height: 100%;border-radius: 10rpx}
.head_top_userHead_vip{position: absolute;bottom: -10rpx;right: -10rpx;width: 34rpx;height: 34rpx;}
.head_top_userName{margin-left: 36rpx}
.head_top_userName_name{width: 300rpx;line-height: 62rpx;font-size: 32rpx;color: #fff}
.head_top_userName_depict{width: 350rpx;font-size: 26rpx;color: rgba(255, 255, 255, .5)}
.head_top_userFollow{margin-top: 17rpx;width: 142rpx;height: 52rpx;border: 1px solid #fff;line-height: 52rpx;border-radius: 26rpx;font-size: 30rpx;color: #fff}
.head_top_iconLine{margin: 26rpx 44rpx 0;width: 2rpx;height: 60rpx;background-color: #689dff}
.head_bottom{box-sizing: border-box;padding: 30rpx 0 30rpx;height: 150rpx;background-color: #145ce3}
.head_bottom_box{width: 33%}
.head_bottom_box:nth-child(2){border-left: 2rpx solid #689dff;border-right: 2rpx solid #689dff}
.head_bottom_box_number{margin-bottom: 16rpx;line-height: 44rpx;font-size: 44rpx;font-weight: 500;color: #fff}
.head_bottom_box_depict{font-size: 26rpx;color: rgba(255, 255, 255, .5)}
/* 标题 */
.title{border-bottom: 1px solid #ddd;background-color: #fff}
.title>view{padding: 30rpx 0 30rpx;width: 33.3%;text-align: center}
.title>view>view:nth-child(1){margin-bottom: 20rpx;font-size: 36rpx;color: #2e2e2e;line-height: 36rpx}
.title>view>view:nth-child(2){font-size: 30rpx;color: #808080;line-height: 26rpx}
/* 内容 */
.section{padding: 36rpx 30rpx 30rpx;background-color: #fff;margin: 20rpx 0}
.section>view{margin-bottom:30rpx}
.section>view:last-child{margin: 0}
.section>view>view:nth-child(1){padding-right: 30rpx;font-size: 30rpx;color: #b9b9b9;line-height: 30rpx}
.section>view>view:nth-child(2){width: 566rpx;font-size: 30rpx;color: #808080;line-height: 30rpx}
/* 数据 */
.data{background-color: #fff;padding:0 24rpx;}
.data>view{margin-right: 34rpx;width: 151rpx;height: 136rpx;text-align: center;border-radius: 20rpx}
.data>view:last-child{margin: 0}
.data>view>view:nth-child(1){margin: 30rpx 0 20rpx;font-size: 30rpx;color: #fc7946; line-height: 30rpx}
.data>view>view:nth-child(2){font-size: 26rpx;color: #b9b9b9;line-height: 26rpx}
/* 阅读 */
.read{padding: 34rpx 30rpx 30rpx;background-color: #fff}
.read>view:nth-child(1){margin-bottom: 30rpx;font-size: 34rpx;color: #2e2e2e;line-height: 34rpx;text-align: left}
.read>view:nth-child(1) text{font-size: 28rpx;color: #b9b9b9;}
.read>view:nth-child(2) image{width: 82rpx;height: 82rpx;border-radius: 50%;border: 2px solid #fff}
.read>view:nth-child(2) image:nth-child(2){margin-left: -22rpx}
.read>view:nth-child(2) image:nth-child(3){margin-left: -22rpx}
.read>view:nth-child(2) image:nth-child(4){margin-left: -22rpx}
.read>view:nth-child(2) image:nth-child(5){margin-left: -22rpx}
.read>view:nth-child(2) image:nth-child(6){margin-left: -22rpx}
.read>view:nth-child(2) image:nth-child(7){margin-left: -22rpx}
.read>view:nth-child(2) image:nth-child(8){margin-left: -22rpx}
.read>view:nth-child(2) image:nth-child(9){margin-left: -22rpx}
.read>view:nth-child(2) view{font-size: 0;padding-top: 34rpx;margin-left: 8rpx;height: 95rpx;}
.read>view:nth-child(2) view text{margin: 0 6rpx;display: inline-block;width: 14rpx;height: 14rpx;background-color: #dddddd;border-radius: 50%}
/* 打赏 */
.btn{margin-top: 40rpx;height: 100rpx;background-color: #fff;line-height: 100rpx;text-align: center;font-size: 34rpx;color: #2772ff}
.btn_dashang{ width: 300rpx;height:80rpx;margin: 0 auto;border-radius: 40rpx;background:#2772ff; color:#fff;line-height:80rpx;text-align: center; font-size: 34rpx;}
/* 遮罩 */
.rewardmask>view{position: absolute;top: 15%;left: 50%;margin-left: -244rpx;width: 488rpx;height: 690rpx;background-color: #f4ebd4;border-radius: 20rpx;font-size: 0}
.rewardmask>view>image:nth-child(1){width: 100%;height: 272rpx}
.rewardmask>view>view:nth-child(2){padding: 0 35rpx 10rpx 70rpx}
.rewardmask>view>view:nth-child(2)>view{box-sizing: border-box;margin: 0 30rpx 30rpx 0;width: 160rpx;height: 60rpx;border: 1px solid #e32f37;font-size: 32rpx;color: #e32f37;line-height: 56rpx;text-align: center;border-radius: 6rpx}
/* .rewardmask>view>view:nth-child(2)>view:nth-child(3),.mask>view>view>view:nth-child(6){margin-right: 0} */
.rewardmask>view>view:nth-child(3){margin-bottom: 15rpx;text-align: center;font-size: 26rpx;color: #808080;line-height: 26rpx}
.rewardmask>view>view:nth-child(4){margin: 0 auto;width: 300rpx;height: 60rpx;background-color: #e32f37;text-align: center;font-size: 30rpx;color: #fff;line-height: 60rpx}
.rewardmask>view>image:last-child{position: absolute;bottom: -74rpx;left: 50%;margin-left: -23.5rpx;width: 47rpx;height: 47rpx}
/* 提示 */
.cue{padding: 30rpx;font-size: 0}
.cue image{margin-right: 20rpx;width: 37rpx;height: 37rpx;vertical-align: middle}
.cue text{font-size: 26rpx;color: #fc7946;vertical-align: middle}
/* 支付 */
.pay{margin-top: 10rpx;height: 100rpx;background-color: #3f86ff;line-height: 100rpx;text-align: center;font-size: 34rpx;color: #ffffff}
/* 遮罩 */
.paymask>view{position: absolute;top: 20%;left: 50%;margin-left: -244rpx;width: 500rpx;height: 433rpx;background-color: #fff;border-radius: 20rpx;}
.paymask>view>image:nth-child(1){width: 100%;height: 290rpx}
.paymask>view>view:nth-child(2){position: absolute;top: 115rpx;left: 50%;transform: translateX(-50%);font-size: 0;width: auto;text-overflow:ellipsis; white-space:nowrap;}
.paymask>view>view:nth-child(2)>image{margin-right: 12rpx;width: 84rpx;height: 117rpx}
.paymask>view>view:nth-child(2)>image:first-child{position: absolute;top: -25rpx;left: -65rpx;width: 54rpx;height: 50rpx}
.paymask>view>view:nth-child(2)>image:last-child{position: absolute;top: -20rpx;right: -74rpx;width: 83rpx;height: 76rpx}
.paymask>view>view:nth-child(2)>view{box-sizing: border-box;margin: 0 30rpx 30rpx 0;width: 120rpx;height: 60rpx;border: 1px solid #e32f37;font-size: 32rpx;color: #e32f37;line-height: 56rpx;text-align: center;border-radius: 6rpx}
.paymask>view>view:nth-child(2)>view:nth-child(3),.mask>view>view>view:nth-child(6){margin-right: 0}
.one{width: 51rpx!important}
.paymask>view>view:nth-child(3){margin-top: 27rpx}
.paymask>view>view:nth-child(3) view{box-sizing: border-box;width: 196rpx;height: 71rpx;font-size: 30rpx;color: #ffffff;background-color: #fc7946;border-radius: 35.5rpx;line-height: 71rpx;text-align: center}
.paymask>view>view:nth-child(3) view:nth-child(1){margin: 0 42rpx 0 32rpx;color: #dddddd;background-color: #fff;border: 1px solid #dddddd}
