.title{padding: 40rpx 30rpx 20rpx;font-size: 30rpx;line-height: 30rpx;color: #939393}
.bb{border-bottom: 1rpx solid #eeeeee}
.box{box-sizing: border-box;padding: 0 30rpx;width: 100%;height: 100rpx;background-color: #fff}
.box>view{box-sizing: border-box;padding: 32rpx 0;width: 100%;height: 100%;font-size: 0}
.box>view>view:nth-child(1){font-size: 32rpx;line-height: 32rpx;color: #333333}
.box>view>image:nth-child(2){width: 19rpx;height: 34rpx}
.other{box-sizing: border-box;padding: 40rpx 30rpx 0;width: 100%;background-color: #fff}
.other:last-child{padding-bottom: 35rpx}
.other>view{font-size: 28rpx;color: #2e2e2e}
.other>view:first-child{margin-bottom: 10rpx;color: #2772ff}
rich-text{color: #333;font-size: 28rpx}