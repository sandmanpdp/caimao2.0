/* pages/master/master.wxss */
/* 头部 */
.head{height: 306rpx}
.head_top{box-sizing: border-box;padding: 40rpx 48rpx 38rpx;height: 184rpx;background-color: #2772ff}
.head_top_userHead{position: relative;width: 102rpx;height: 102rpx;border-radius: 10rpx;background-color: #fff;border: 2rpx solid #fff}
.head_top_userHead_img{width: 100%;height: 100%;border-radius: 10rpx}
.head_top_userHead_vip{position: absolute;bottom: -10rpx;right: -10rpx;width: 34rpx;height: 34rpx;}
.head_top_userName{margin-left: 36rpx}
.head_top_userName_name{width: 320rpx;line-height: 62rpx;font-size: 32rpx;color: #fff}
.head_top_userName_depict{width: 342rpx;font-size: 26rpx;color: rgba(255, 255, 255, .5)}
.head_top_follow{margin-top: 24rpx;width: 142rpx;height: 52rpx;line-height: 52rpx;border: 2rpx solid #fff;border-radius: 26rpx;font-size: 30rpx;color: #fff}
.head_bottom{box-sizing: border-box;padding: 20rpx 0 0;height: 120rpx;background-color: #145ce3}
.head_bottom_box{width: 33%}
.head_bottom_box:nth-child(2){border-left: 2rpx solid #689dff;border-right: 2rpx solid #689dff}
.head_bottom_box_number{margin-bottom: 8rpx;line-height: 42rpx;font-size: 34rpx;font-weight: 400;color: #fff}
.head_bottom_box_depict{font-size: 24rpx;color: rgba(255, 255, 255, .5)}
.head_bottom_box_depict_icon{display: inline-block;position: relative;top: -2rpx;margin-left: 4rpx;width: 26rpx;height: 26rpx;border: 2rpx solid rgba(255, 255, 255, .5);border-radius: 50%;text-align: center;line-height: 24rpx;font-size: 22rpx}
.head_quiz{box-sizing: border-box;padding: 23rpx 0;height: 120rpx;background-color: #fff}
.head_quiz_box{box-sizing: border-box;margin: 0 auto;padding: 16rpx 47rpx;width: 272rpx;height: 74rpx;line-height: 74rpx;border: 2rpx solid #35d2af;border-radius: 37rpx}
.head_quiz_box_icon{width: 37rpx;height: 38rpx}
.head_quiz_box_title{margin-left: 12rpx;line-height: 38rpx;font-size: 32rpx;color: #808080}
/* 主体 */
.section{margin-top: 20rpx}
.section_nav{height: 98rpx;background-color: #fff;border-bottom: 4rpx solid #dddddd}
.section_nav_box{width: 50%;line-height: 98rpx;text-align: center;font-size: 34rpx;color: #b9b9b9}
.section_nav_true{color: #2772ff}
.section_show_box{margin-top: 20rpx;padding-bottom: 26rpx;background-color: #fff}
.section_show_box:first-child{margin-top: 0}
.section_show_notes_title{padding: 0 30rpx;height: 88rpx;line-height: 88rpx;border-bottom: 2rpx solid #dddddd}
.section_show_notes_title_text{margin-right: 30rpx;font-size: 34rpx;color: #141414;font-weight: 400}
.section_show_notes_title_price{font-size: 34rpx;color: #fc7946}
.section_show_notes_title_state{font-size: 26rpx;color: #2772ff}
.section_show_bottom_top{margin: 34rpx 0;width: 50%;height: 82rpx;text-align: center}
.section_show_bottom_top:first-child{box-sizing: border-box;border-right: 2rpx solid #dddddd}
.section_show_bottom_top_title{margin-bottom: 18rpx;line-height: 38rpx;font-size: 40rpx;color: #e32f37;font-weight: 600}
.section_show_bottom_top_depict{line-height: 24rpx;font-size: 24rpx;color: #808080}
.section_show_bottom_bottom{padding: 0 30rpx;line-height: 22rpx}
.section_show_bottom_bottom_left{font-size: 26rpx;color: #b9b9b9}
.section_show_bottom_bottom_right_icon{margin-right: 18rpx;width: 27rpx;height: 22rpx}
.section_show_bottom_bottom_right_number{font-size: 26rpx;color: #b9b9b9}
.section_show_view_title{box-sizing: border-box;position: relative;padding: 30rpx 30rpx 0}
.section_show_view_title_icon{margin-right: 20rpx;width: 72rpx;height: 46rpx;line-height: 46rpx;border: 2rpx solid #ffad2b;border-radius: 10rpx;font-size: 30rpx;text-align: center;color: #ffad2b}
.section_show_view_title_text{margin-right: 20rpx;line-height: 50rpx;font-size: 34rpx;color: #141414;font-weight: 400}
.section_show_view_title_number{line-height: 50rpx;font-size: 34rpx;color: #b9b9b9}
.section_show_view_title_price{line-height: 50rpx;font-size: 34rpx;color: #fc7946}
.section_show_view_title_state{position: absolute;top: 50%;right: 0;margin-top: -25rpx;padding-left: 24rpx;width: 152rpx;height: 50rpx;line-height: 50rpx;font-size: 24rpx;color: #fff;background-color: #ffad2b;border-top-left-radius: 25rpx;border-bottom-left-radius: 25rpx;}
.section_show_view_bottom_depict{margin: 42rpx 30rpx 32rpx;font-size: 28rpx;color: #808080}
.section_show_view_bottom_jump,.section_show_view_bottom_time{margin: 0 0 0 30rpx ;font-size: 26rpx;color: #b9b9b9}
.section_show_askTheStock_title_icon{margin-right: 20rpx;width: 46rpx;height: 46rpx;line-height: 46rpx;border: 2rpx solid #2772ff;border-radius: 10rpx;font-size: 30rpx;text-align: center;color: #2772ff}

/* 颜色状态 */
.fc_gray{color: #b9b9b9;border-color: #b9b9b9}
.bc_gray{background-color: #b9b9b9}


.box_box{padding: 0 30rpx;background-color: #fff}
.box_box>.box{padding: 40rpx 0 30rpx;border-bottom: 1px solid #eeeeee}
.box_box>.box:last-child{border: 0}
.box .title{font-size: 36rpx;color: #2e2e2e;font-weight: 600}
.box .text{margin: 25rpx 0 38rpx;line-height: 50rpx;font-size: 28rpx;color: #808080}
.box .info,.data{font-size: 0}
.box .info .author{font-size: 26rpx;color: #b9b9b9}
.box .info .headimg{margin:0 20rpx 0 0;width:49rpx;height:49rpx;border-radius:6rpx;vertical-align: middle}
.box .info .name{display: inline-block;font-size: 26rpx;color: #808080}
.data .icon{margin-right: 20rpx;width: 40rpx;height: 35rpx;vertical-align: middle}
.data .number{font-size: 24rpx;color: #808080;vertical-align: middle}
.data .number:nth-child(4){margin-right: 30rpx}
.data .number:nth-child(2){margin-right: 49rpx}


.issue image{display: block;margin: 40rpx auto;width: 252rpx;height: 246rpx}
 /* by pdp */
.rateIsMinus {
  color: #42c110 !important;
}
.rateIsPositive {
  color: #e32f37 !important;
}
.yuanbao {
  width: 40rpx;
  height: 30rpx;
}