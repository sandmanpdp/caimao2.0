.coe32f37{color: #e32f37!important;font-weight: 600}
.cofc7946{color: #fc7946!important}
.co2772ff{color: #2772ff!important}
.cof4ebd4{color: #f4ebd4!important;background-color: #e32f37}
.lht45{line-height: 45rpx!important}
/* 头部 */
.head{height: 304rpx}
.head_top{box-sizing: border-box;padding: 40rpx 48rpx 38rpx;height: 184rpx;background-color: #2772ff}
.head_top_userHead{position: relative;width: 102rpx;height: 102rpx;border-radius: 10rpx;background-color: #fff;border: 2rpx solid #fff}
.head_top_userHead_img{width: 100%;height: 100%;border-radius: 10rpx}
.head_top_userHead_vip{position: absolute;bottom: -10rpx;right: -10rpx;width: 34rpx;height: 34rpx;}
.head_top_userName{margin-left: 36rpx}
.head_top_userName_name{width: 330rpx;line-height: 62rpx;font-size: 32rpx;color: #fff}
.head_top_userName_depict{width: 210rpx;font-size: 26rpx;color: rgba(255, 255, 255, .5)}
.head_top_userFollow{margin-top: 24rpx;width: 142rpx;height: 52rpx;border: 1px solid #fff;line-height: 52rpx;border-radius: 26rpx;font-size: 30rpx;color: #fff}
.head_top_iconLine{margin: 26rpx 44rpx 0;width: 2rpx;height: 60rpx;background-color: #689dff}
.head_bottom{box-sizing: border-box;padding: 20rpx 0 0;height: 120rpx;background-color: #145ce3}
.head_bottom_box{width: 33%}
.head_bottom_box:nth-child(2){border-left: 2rpx solid #689dff;border-right: 2rpx solid #689dff}
.head_bottom_box_number{margin-bottom: 8rpx;line-height: 42rpx;font-size: 34rpx;font-weight: 400;color: #fff}
.head_bottom_box_depict{font-size: 24rpx;color: rgba(255, 255, 255, .5)}
/* 标题 */
.title{margin-bottom: 10rpx;background-color: #fff}
.title>view{padding: 30rpx 0 25rpx;width: 33.3%;text-align: center}
.title>view>view:nth-child(1){margin-bottom: 15rpx;font-size: 30rpx;color: #2e2e2e;line-height: 30rpx}
.title>view>view:nth-child(2){font-size: 26rpx;color: #b9b9b9;line-height: 26rpx}
/* 内容 */
.section{padding: 50rpx 30rpx 30rpx;background-color: #fff;border-bottom: 1px solid #ddd}
.section>view{margin-bottom: 41rpx}
.section>view:last-child{margin: 0}
.section>view>view:nth-child(1){padding-left: 21rpx;font-size: 30rpx;color: #b9b9b9;line-height: 30rpx}
.section>view>view:nth-child(2){width: 566rpx;font-size: 30rpx;color: #808080;line-height: 30rpx}
/* 阅读 */
.read{padding: 30rpx;background-color: #fff}
.read>view:nth-child(1){margin-bottom: 30rpx;font-size: 34rpx;color: #2e2e2e;line-height: 34rpx}
.read>view:nth-child(1) text{font-size: 28rpx;color: #b9b9b9}
.read>view:nth-child(2) image{width: 82rpx;height: 82rpx;border-radius: 50%;border: 2px solid #fff}
.read>view:nth-child(2) image:nth-child(2){margin-left: -22rpx}
.read>view:nth-child(2) image:nth-child(3){margin-left: -22rpx}
.read>view:nth-child(2) image:nth-child(4){margin-left: -22rpx}
.read>view:nth-child(2) image:nth-child(5){margin-left: -22rpx}
.read>view:nth-child(2) image:nth-child(6){margin-left: -22rpx}
.read>view:nth-child(2) image:nth-child(7){margin-left: -22rpx}
.read>view:nth-child(2) image:nth-child(8){margin-left: -22rpx}
.read>view:nth-child(2) view{margin: 34rpx 0 0 8rpx;font-size: 0}
.read>view:nth-child(2) view text{margin: 0 6rpx;display: inline-block;width: 14rpx;height: 14rpx;background-color: #dddddd;border-radius: 50%}
/* 提示 */
.cue{padding: 30rpx;font-size: 0}
.cue image{margin-right: 20rpx;width: 37rpx;height: 37rpx;vertical-align: middle}
.cue text{font-size: 26rpx;color: #fc7946;vertical-align: middle}
/* 支付 */
.btn{margin-top: 10rpx;height: 100rpx;background-color: #3f86ff;line-height: 100rpx;text-align: center;font-size: 34rpx;color: #ffffff}
/* 遮罩 */
.mask>view{position: absolute;top: 20%;left: 50%;margin-left: -244rpx;width: 500rpx;height: 433rpx;background-color: #fff;border-radius: 20rpx;}
.mask>view>image:nth-child(1){width: 100%;height: 290rpx}
.mask>view>view:nth-child(2){position: absolute;top: 115rpx;left: 50%;transform: translateX(-50%);font-size: 0;width: auto;text-overflow:ellipsis; white-space:nowrap;}
.mask>view>view:nth-child(2)>image{margin-right: 12rpx;width: 84rpx;height: 117rpx}
.mask>view>view:nth-child(2)>image:first-child{position: absolute;top: -25rpx;left: -65rpx;width: 54rpx;height: 50rpx}
.mask>view>view:nth-child(2)>image:last-child{position: absolute;top: -20rpx;right: -74rpx;width: 83rpx;height: 76rpx}
.mask>view>view:nth-child(2)>view{box-sizing: border-box;margin: 0 30rpx 30rpx 0;width: 120rpx;height: 60rpx;border: 1px solid #e32f37;font-size: 32rpx;color: #e32f37;line-height: 56rpx;text-align: center;border-radius: 6rpx}
.mask>view>view:nth-child(2)>view:nth-child(3),.mask>view>view>view:nth-child(6){margin-right: 0}
.one{width: 51rpx!important}
.mask>view>view:nth-child(3){margin-top: 27rpx}
.mask>view>view:nth-child(3) view{box-sizing: border-box;width: 196rpx;height: 71rpx;font-size: 30rpx;color: #ffffff;background-color: #fc7946;border-radius: 35.5rpx;line-height: 71rpx;text-align: center}
.mask>view>view:nth-child(3) view:nth-child(1){margin: 0 42rpx 0 32rpx;color: #dddddd;background-color: #fff;border: 1px solid #dddddd}
.replenishViews {
  padding:0 30rpx 10rpx;
  background-color:#fff;
  margin-bottom:10rpx;
  margin-top: 10rpx;
}
.replenishViews_title {
  padding: 18rpx 0;
  border-bottom: 1px solid #ddd;
  font-size: 32rpx;
}
.viewTitle {
  padding: 14rpx 0;
  font-size: 30rpx;
  color:#808080;
}
.viewDetails,.viewTime {
  color: #BCBCBC;
  font-size: 26rpx;
}
.viewDetails {
  line-height: 40rpx;
}
.viewTime {
  padding: 23rpx 0 10rpx 0;
  height: 65rpx;
}
.buyBtn {
  position: fixed;
  bottom: 0;
  width: 100%;
}