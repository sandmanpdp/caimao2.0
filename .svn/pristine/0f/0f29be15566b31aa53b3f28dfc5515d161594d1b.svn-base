<!--pages/component/auth/auth.wxml-->
<view class="wrapper">
  <view class='alertBox'>
    <view class='alertTitle'>
      登陆授权
    </view>
    <view class='content'>
      <view class='logo'>
        <image class='logoImg' src='../../images/integral_nav4.png'></image>
      </view>
      <view class='contentTitle'>
        股先森申请获得以下权限:
      </view>
      <view class='contentDetail'>
        获取你的公开信息（昵称、头像等）
      </view>
    </view>
    <view class='btnBox'>
      <view class='denyBtn'>
        拒绝
        <button class='btn'></button>
      </view>
      <view class='commitBtn'>
        接受
        <button class='btn' open-type='getUserInfo' bindgetuserinfo="onGotUserInfo"></button>
      </view>
    </view>
  </view>
  <slot></slot>
</view>
